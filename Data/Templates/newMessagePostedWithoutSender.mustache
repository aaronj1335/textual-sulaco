<div type="{{lineType}}" class="line {{lineClassAttributeRepresentation}}" id="line{{lineNumber}}" command="{{command}}">
    <span class="sender"></span>
	<span class="message" type="{{lineType}}">
		{{{formattedMessage}}}
	</span>
    <span class="time" title={{localizedTimestamp}}>{{formattedTimestamp}}</span>
</div>
